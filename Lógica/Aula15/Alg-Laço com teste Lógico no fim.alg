algoritmo "Laço com Teste Lógico no Fim"
// Função : Exemplo
// Autor :  Fabricio Curvello Gomes
// Data : 18/11/2015
// Seção de Declarações 
var
   R, X, I: inteiro
inicio
// Seção de Comandos 
I <- 1
repita
      escreval("Digite um numero: ")
      leia(x)
      R <- X * 3
      escreval (R)
      I <- I + 1
ate (I > 5)
fimalgoritmo
