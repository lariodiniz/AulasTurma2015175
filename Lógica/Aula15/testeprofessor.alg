algoritmo "semnome"
// Função :
// Autor :
// Data : 18/11/2015
// Seção de Declarações 
var
p: caracter
i, x : inteiro
inicio
escreval("Informe uma palavra: ")
leia(p)
i <- 1
x <- 1
enquanto ( i < 10 ) faca
         enquanto (x < i) faca
                  escreva(p)
         fimenquanto
         escreval("")
fimenquanto
         
fimalgoritmo
