algoritmo "semnome"
// Função :
// Autor :
// Data : 11/11/2015
// Seção de Declarações 
var
 aumento, salario : real
inicio
// Seção de Comandos 
 escreva("Informe seu Salario: ")
 leia(salario)
 aumento <- salario + ((salario/100)*15)
 escreva("Seu sarario com aumento é de:" ,aumento)
fimalgoritmo
